// calc grammar
imports:
 
priorities:

tokens:
	adType:String 	=	'flash|static|dynamic'
	size:String		=	'[0-9]{2,3}x[0-9]{2,3}'
	bye:String		=	'bye'
 
blanks:
 	space= "( |\t|\r|\n)+"
 
comments:
 	comment="#([^\r\n])*(\r)?\n"  

starts:
 	batch

productions:
	batch
		= advertisement+ 'bye' {batch}
		| advertisement+ {batch};
		
	advertisement
		= 'adType' 'size' {ad};